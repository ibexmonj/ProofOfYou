{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://proofofyou.com/schema/contributions.schema.json",
  "title": "ProofOfYou Contributions v0.1",
  "type": "object",
  "required": ["version", "highlights", "meta"],
  "properties": {
    "version": {"type": "string"},
    "subject": {},
    "period": {"type": "object"},
    "highlights": {"type": "array"},
    "activity_feed": {"type": "array"},
